{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "lib": [
      "dom",
      "es2015"
    ],
    "module": "cjs",
    "target": "es5",
    "declaration": false,
    "noImplicitAny": true,
    "preserveConstEnums": true,
    "removeComments": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "outDir": "../build/test"
  },
  "include": [
    "**/*.ts",
    "../node_modules/@resin.io/**/*.d.ts"
  ]
}
